<div class="author">
    <h1 class="author_heading">Search Results</h1>
</div>

<section class="article">
    <ul class="article_ul">

        <% data.forEach(element=> { %>
            <li>
                <a href="/posts/<%=element._id %>">
                    <span>
                        <%= element.title %>
                    </span>
                    <span class="article-list-date">
                        <%= element.createdAt %>
                    </span>
                </a>
            </li>
            <% }); %>
    </ul>

</section>